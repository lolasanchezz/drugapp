      "spl_unclassified_section_table": [
        "<table width=\"100%\" ID=\"if0175c6e-d833-47a1-8982-3f14a1e2a1f0\"> <caption>TABLE 4 Incidence of Common Adverse Reactions in Patients with Major Depressive Disorder</caption> <col align=\"left\" width=\"32%\"/> <col align=\"left\" width=\"20%\"/> <col align=\"left\" width=\"25%\"/> <col align=\"left\" width=\"21%\"/> <tbody> <tr> <td> <content styleCode=\"bold\">Adverse Reaction</content> </td> <td> <content styleCode=\"bold\">Placebo</content> </td> <td> <content styleCode=\"bold\">10 mg/day</content> </td> <td> <content styleCode=\"bold\">20 mg/day</content> </td> </tr> <tr> <td>   </td> <td> <content styleCode=\"bold\">(N=311)</content> </td> <td> <content styleCode=\"bold\">Lexapro</content> </td> <td> <content styleCode=\"bold\">Lexapro</content> </td> </tr> <tr> <td>   </td> <td>   </td> <td> <content styleCode=\"bold\">(N=310)</content> </td> <td> <content styleCode=\"bold\">(N=125)</content> </td> </tr> <tr> <td> <content styleCode=\"bold\">Insomnia</content> </td> <td>4%</td> <td>7%</td> <td>14%</td> </tr> <tr> <td> <content styleCode=\"bold\">Diarrhea</content> </td> <td>5%</td> <td>6%</td> <td>14%</td> </tr> <tr> <td> <content styleCode=\"bold\">Dry Mouth</content> </td> <td>3%</td> <td>4%</td> <td>9%</td> </tr> <tr> <td> <content styleCode=\"bold\">Somnolence</content> </td> <td>1%</td> <td>4%</td> <td>9%</td> </tr> <tr> <td> <content styleCode=\"bold\">Dizziness</content> </td> <td>2%</td> <td>4%</td> <td>7%</td> </tr> <tr> <td> <content styleCode=\"bold\">Sweating Increased</content> </td> <td>less than 1%</td> <td>3%</td> <td>8%</td> </tr> <tr> <td> <content styleCode=\"bold\">Constipation</content> </td> <td>1%</td> <td>3%</td> <td>6%</td> </tr> <tr> <td> <content styleCode=\"bold\">Fatigue</content> </td> <td>2%</td> <td>2%</td> <td>6%</td> </tr> <tr> <td> <content styleCode=\"bold\">Indigestion</content> </td> <td>1%</td> <td>2%</td> <td>6%</td> </tr> </tbody> </table>",
        "<table width=\"100%\" ID=\"i87ee1b7a-abc7-4f00-80e4-953912f137c1\"> <caption>TABLE 5 Incidence of Sexual Side Effects in Placebo-Controlled Clinical Trials</caption> <col align=\"left\" width=\"35%\"/> <col align=\"left\" width=\"32%\"/> <col align=\"left\" width=\"32%\"/> <tbody> <tr> <td> <content styleCode=\"bold\">Adverse Event</content> </td> <td> <content styleCode=\"bold\">Lexapro</content> </td> <td> <content styleCode=\"bold\">Placebo</content> </td> </tr> <tr> <td>   </td> <td>In Males Only</td> </tr> <tr> <td>   </td> <td>(N=407)</td> <td>(N=383)</td> </tr> <tr> <td>Ejaculation Disorder (primarily ejaculatory delay)</td> <td>  12%</td> <td>  1%</td> </tr> <tr> <td>Libido Decreased</td> <td>6%</td> <td>2%</td> </tr> <tr> <td> Impotence</td> <td>2%</td> <td>less than 1%</td> </tr> <tr> <td>   </td> <td>In Females Only</td> </tr> <tr> <td>   </td> <td>(N=737)</td> <td>(N=636)</td> </tr> <tr> <td>Libido Decreased</td> <td>3%</td> <td>1%</td> </tr> <tr> <td>Anorgasmia</td> <td>3%</td> <td>less than 1%</td> </tr> </tbody> </table>"
